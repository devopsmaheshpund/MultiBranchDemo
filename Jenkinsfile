pipeline{
  agent any
  stages{
    stage('checkout'){
      steps{
        sh 'echo "checkout code"'
      }
    }

    stage('Compile'){
      steps{
        sh 'echo "Compile Code"'
      }
    }

    stage('Test'){
      steps{
        sh 'echo "Test Code"'
      }
    }

    stage('package code'){
      steps{
        sh 'echo "package code"'
      }
    }
  }
}
